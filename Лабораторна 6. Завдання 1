def list_operations(lst):
  
    if len(lst) < 3:
        print("List is too short to perform the operations.")
        return
    
    del lst[1]
    
    del lst[-2]
    
    print("Modified list:", lst)

elements = input("Enter list elements separated by spaces: ")
lst = [int(x) for x in elements.split()]

list_operations(lst)

input("Натисніть Enter, щоб вийти...")
